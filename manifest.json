{
  "name": "Chrome Demo",
  "description": "TBD",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/df-favicon.png",
      "32": "./images/df-favicon.png",
      "48": "./images/df-favicon.png",
      "128": "./images/df-favicon.png"
    }
  },
  "icons": {
    "16": "./images/df-favicon.png",
    "32": "./images/df-favicon.png",
    "48": "./images/df-favicon.png",
    "128": "./images/df-favicon.png"
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
